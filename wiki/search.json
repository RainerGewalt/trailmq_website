---
layout: null
permalink: /wiki/search.json
---
[
  {% for article in site.wiki_articles %}
  {
    "title": {{ article.title | jsonify }},
    "url": {{ article.url | relative_url | jsonify }},
    "description": {{ article.description | default: "" | jsonify }},
    "category": {{ article.category | default: "" | jsonify }},
    "tags": {{ article.tags | default: "" | jsonify }},
    "date": {{ article.date | date: "%Y-%m-%d" | jsonify }}
  }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]
